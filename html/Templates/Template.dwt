    <script type="text/javascript">
		$(document).ready(function () {
			$("#arrivalDate").datepicker({
				changeMonth: true,
				changeYear: true,
				yearRange: "+0:+10",
				dateFormat: "yyyy-mm-dd"
			});
		});
	
		function goToBookingPage() {
			var DateInStr   = document.getElementById('arrivalDate').value;
			var arrivalDate = document.getElementById('arrivalDate').value;
			var splitDate = arrivalDate.split("../index.html");
				arrivalDate=splitDate[1]+"/"+splitDate[0]+"/"+splitDate[2]
			var night = document.input.nights.selectedIndex;
			var nights = document.input.nights.options[night].value;	
			var adult = document.input.adults.selectedIndex;
			var adults = document.input.adults.options[adult].value;
			var child = document.input.children.selectedIndex;
			var children = document.input.children.options[child].value;
			var startDate = parseDMY1(DateInStr);
			var DateIn = DatetoStrMDY(startDate);	
			var defval = "https://bookings.ihotelier.com/Pangkor-Laut-Resort/bookings.jsp?HotelID=72663&languageid=11";
			myfile = defval + "&datein="+escape(DateIn) + "&arrivalDate=" + arrivalDate + "&nights=" + nights + "&adults=" + adults + "&children=" + children;
			window.open(myfile, 'bookingwindow');
		}
    </script>
		
    <script type="text/javascript">
		function DatetoStrMDY(d) {
			var d = new Date(d);
			var day = d.getDate();
			var month = d.getMonth() + 1; //months are zero based
			var year = d.getFullYear();		
			return month + "/" + day + "/" + year;
		}
		
		function parseDMY(dstr) {
			var parts = dstr.match(/(\d+)/g);
			return new Date(parts[2], parts[1], parts[0]);
  		}
		
		function parseDMY1(dstr) {
			var parts = dstr.match(/(\d+)/g);
			return new Date(parts[2], parts[1]-1, parts[0]);
  		}
	</script>

